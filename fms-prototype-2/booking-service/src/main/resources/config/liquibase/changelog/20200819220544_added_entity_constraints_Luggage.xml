<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity Luggage.
    -->
    <changeSet id="20200819220544-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="booking_id"
                                 baseTableName="luggage"
                                 constraintName="fk_luggage_booking_id"
                                 referencedColumnNames="id"
                                 referencedTableName="booking"/>

        <addForeignKeyConstraint baseColumnNames="flight_id"
                                 baseTableName="luggage"
                                 constraintName="fk_luggage_flight_id"
                                 referencedColumnNames="id"
                                 referencedTableName="flight"/>

    </changeSet>
</databaseChangeLog>
